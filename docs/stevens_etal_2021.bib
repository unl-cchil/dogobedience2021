@article{Kwan.Bain.2013,
  title = {Owner Attachment and Problem Behaviors Related to Relinquishment and Training Techniques of Dogs},
  author = {Kwan, Jennifer Y and Bain, Melissa J},
  year = {2013},
  volume = {16},
