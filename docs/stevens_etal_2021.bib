  issn = {0892-7936},
  doi = {10.2752/175303713X13697429463673},
  abstract = {We investigated the extent to which personality match between dog and owner is related to pet satisfaction. Prior research shows that owners relinquish dogs at a high rate, not only in the United States, but also in other countries. New procedures to match owners to future pets could reduce the problem of homeless pets. These were developed after examining the literature to determine all possible traits in dogs and conducting structured interviews to check for further traits. After doing this, 45 traits were determined, and two surveys were created. One survey assessed the personality traits of the dog, and the other assessed the owner's personality traits. In our study, 88 dog owners were asked to report their satisfaction with their self-selected dog and to describe their dog's personality characteristics, based on eight dimensions. Correlations between dog satisfaction scores and dog\textendash owner personality match revealed four characteristics that were significantly associated with satisfaction: tendency to share possessions, love of running outside, likeliness of being destructive, and ability to get along with others. These results suggest that prospective dog owners may want to consider adopting dogs who match their personality on these characteristics. Matching the personality of dogs and their owners on a few characteristics may predict owner satisfaction. This is important as it may reduce the number of dogs that are relinquished by owners as well as the number of dogs left homeless each year.},
  annotation = {\_eprint: https://doi.org/10.2752/175303713X13697429463673},
  file = {/home/jstevens/zotero-library/Curb et al/curb_et_al_2013_the_relationship_between_personality_match_and_pet_satisfaction_among_dog_owners.pdf},
  journal = {Anthrozo\"os},
  keywords = {canine,human,matching,personality},
  number = {3}
}

@phdthesis{Daye.2011,
  title = {Dog Owner Personality, Owner Interaction with Dog, and Dog Aggression},
  author = {Daye, Melanie},
  year = {2011},
  address = {{United States -- California}},
  abstract = {The purpose of this study was to examine the relationships between dog owner personality, interactions of the dog owners with their dogs, and aggression displayed by their dogs in different situations. The present study focused on identifying characteristics related to Pit Bull dog owners' personalities, aggression used in interacting with their Pit Bull dogs, and the aggression displayed by their dogs. Four hundred and ninety two adult Pit Bull type dog owners completed an online survey. Results showed that the more aggressive owners' personalities were, the more their dogs displayed aggression toward familiar and unfamiliar dogs, and the less the owners' reported the dogs as trainable. Also, the more aggressive owners were, the more aggressive their interactions with their dogs were. In addition, aggressive owner interactions were related to more dog aggression toward unfamiliar people, familiar and unfamiliar dogs and the dogs were also seen by their owners as less trainable. Finally multiple regression analyses showed that both aggressive owner personality and aggressive behaviors toward the dogs negatively predicted dog trainability. Owner aggressive interactions with the dogs were found to be a positive predictor of dog aggression directed toward unfamiliar and familiar dogs, but owner personality was not a predictor for these types of dog aggression. This study provided exciting new information concerning owner and dog aggression in the context of their relationships with one another and concluded that Pit Bull owner characteristics and their actions may play a role in their dogs' aggressive behaviors, thus showing the need for measures supporting responsible dog ownership.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  file = {/home/jstevens/zotero-library/Daye/daye_2011_dog_owner_personality,_owner_interaction_with_dog,_and_dog_aggression.pdf},
  isbn = {9781124589619},
  keywords = {Dog aggression,Dog owner personality,Owner-dog interaction,Pit bulls,Psychology},
  language = {English},
  school = {Alliant International University},
  type = {Ph.{{D}}. Thesis}
}

